    "m": { "descend": "Moon", "location": "Moon", "fixture": "T" },
    "f": { "descend": "Fawn", "location": "Fawn", "fixture": "T" },
    "E": { "descend": "Exodus", "location": "Exodus", "fixture": "C" },
    "K": {
