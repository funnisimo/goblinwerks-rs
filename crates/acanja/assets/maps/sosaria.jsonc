{
  "$schema": "../map.schema.json",
  "id": "SOSARIA",
  "map": { "filename": "sosaria.txt", "width": 64, "height": 64, "wrap": true },
  "font": "DEFAULT",
  "camera": { "size": [11, 11] },
  "fov": 11,
